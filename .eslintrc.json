{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "plugins": ["jest"],
  "extends": ["airbnb-base","plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ],
    "global-require": 0,
    "import/no-extraneous-dependencies": "off",
    "camelcase": "off",
    "max-classes-per-file": "off"
  }
}